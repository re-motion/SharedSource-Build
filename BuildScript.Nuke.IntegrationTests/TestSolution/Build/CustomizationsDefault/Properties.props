<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <AssemblyInfoFile>$(SolutionDirectory)AssemblyInfoShared.*</AssemblyInfoFile>
    <CompanyName>rubicon IT GmbH</CompanyName>
    <CompanyUrl>http://www.rubicon.eu</CompanyUrl>
    <Copyright>Copyright (c) rubicon IT GmbH, www.rubicon.eu - Licensed under Apache License, Version 2.0</Copyright>
    <ProductName>re-motion Build Infrastructure (www.re-motion.org)</ProductName>
    <DocumentationFileName>Remotion</DocumentationFileName>
    <DocumentationRootPage>$(SolutionDirectory)\Build\Customizations\GettingStarted.aml</DocumentationRootPage>
    <DocumentationNamespaceSummaryFiles>$(SolutionDirectory)\Build\Customizations\*.NamespaceDoc.xml</DocumentationNamespaceSummaryFiles>
    <DependDBProjectName>Testsolution</DependDBProjectName>
    <DependDBRetentionTime>30</DependDBRetentionTime>
    <DependDBProjectBranch>Main</DependDBProjectBranch>
    <!-- Required, but can be supplied via commandline parameter in server build and uses a fallback in local build -->
    <DependDBProjectImportNotificationMailAddress>
    </DependDBProjectImportNotificationMailAddress>
    <DependDBTrackedReferences>Remotion;Remotion.*</DependDBTrackedReferences>
    <DependDBNuGetSources>$(SolutionDirectory)packages</DependDBNuGetSources>
    <DependDBTargetFramework>netstandard2.1</DependDBTargetFramework>
  </PropertyGroup>
</Project>