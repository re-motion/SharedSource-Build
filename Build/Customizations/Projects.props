<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <NormalTestConfiguration Condition="'$(NormalTestConfiguration)' == ''">
      LocalMachine + NET45 + NoBrowser + NoDB + Debug     + x86;
      LocalMachine + NET45 + NoBrowser + NoDB + Release   + x64;
      LocalMachine + NET45 + NoBrowser + NoDB + Debug     + x86;
    </NormalTestConfiguration>
  </PropertyGroup>
  <ItemGroup>
    <ReleaseProjectFiles Include="$(SolutionDirectory)\BuildScript\Buildscript.csproj" />
    <ReleaseProjectFiles Include="$(SolutionDirectory)\ReleaseProcessScript\ReleaseProcessScript.csproj" />
    <ReleaseProjectFiles Include="$(SolutionDirectory)\BuildScript.Nuke\BuildScript.Nuke.csproj" />
    <UnitTestProjectFiles Include="$(SolutionDirectory)\BuildScript.UnitTests\BuildScript.UnitTests.csproj">
      <TestConfiguration>$(NormalTestConfiguration)</TestConfiguration>
    </UnitTestProjectFiles>
    <UnitTestProjectFiles Include="$(SolutionDirectory)\ReleaseProcessScript.UnitTests\ReleaseProcessScript.UnitTests.csproj">
      <TestConfiguration>$(NormalTestConfiguration)</TestConfiguration>
    </UnitTestProjectFiles>
    <UnitTestProjectFiles Include="$(SolutionDirectory)\BuildScript.Nuke.UnitTests\BuildScript.Nuke.UnitTests.csproj">
      <TestConfiguration>$(NormalTestConfiguration)</TestConfiguration>
    </UnitTestProjectFiles>
    <IntegrationTestProjectFiles Include="$(SolutionDirectory)\BuildScript.Nuke.IntegrationTests\BuildScript.Nuke.IntegrationTests.csproj">
      <TestConfiguration>$(NormalTestConfiguration)</TestConfiguration>
    </IntegrationTestProjectFiles>
  </ItemGroup>
</Project>